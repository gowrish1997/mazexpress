{
    "compilerOptions": {
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "target": "es5",
      "lib": ["es5", "es6", "dom"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["./*"],
        "react-native-sqlite-storage": [
          "lib/fix_typeorm_require.ts"
        ]
      }
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "lib/adapter/data-source.zwei.ts", "lib/adapter/data-source.zwei.ts"],
    "exclude": ["node_modules"]
  }
  